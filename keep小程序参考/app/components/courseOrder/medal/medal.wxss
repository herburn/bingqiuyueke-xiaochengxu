.keep-right-more-arrow-new {
    background-image: url("https://static1.keepcdn.com/2019/09/30/15/1569827395455_24x51.png");
    background-repeat: no-repeat;
    background-size: 16rpx 34rpx;
    background-position: center right;
}

.keep-location-icon-black {
    background-image: url("data:img/jpg;base64,");
    background-size: 32rpx 32rpx;
    background-position: center left;
    background-repeat: no-repeat;
}

.keep-clock-icon-black {
    background-image: url("data:img/jpg;base64,");
    background-size: 32rpx 32rpx;
    background-position: center left;
    background-repeat: no-repeat;
}

.keep-1px-border {
    position: relative;
}

.keep-1px-border:after {
    position: absolute;
    left: 0;
    right: 0;
    content: '';
    height: 2rpx;
    transform: scaleY(0.5);
    background-color: #efefef;
}

.keep-1px-border.bottom-border:after {
    bottom: 0;
    transform-origin: 0% 100%;
}

.keep-1px-border.top-border:after {
    top: 0;
    transform-origin: 0% 0%;
}

.keep-text-overflow {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.keep-avatar-placeholder {
    background-image: url("data:img/jpg;base64,");
    background-size: cover;
    background-repeat: no-repeat;
}

.keep-course-cover-placeholder {
    background-color: #efefef;
}

.keep-course-cover-mark {
    position: relative;
}

.keep-course-cover-mark:after {
    content: '';
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background-image: linear-gradient(0deg,rgba(88,79,96,0.8) 0,rgba(88,79,96,0) 100%);
}

.keep-star {
    width: 28rpx;
    height: 28rpx;
    background-repeat: no-repeat;
    background-size: cover;
}

.keep-star.all-star {
    background-image: url("https://static1.keepcdn.com/2019/09/30/15/1569827026382_42x42.png");
}

.keep-star.half-star {
    background-image: url("https://static1.keepcdn.com/2019/09/30/16/1569832043771_42x42.png");
}

.keep-star.empty-star {
    background-image: url("https://static1.keepcdn.com/2019/09/30/16/1569831954695_42x42.png");
}

.keep-open-share {
    display: block;
    margin-top: 12rpx;
    margin-left: 47rpx;
    width: 46rpx;
    height: 46rpx;
    background-color: #fff;
    border-radius: 0;
    background-image: url("data:img/jpg;base64,");
    background-size: 46rpx 46rpx;
    background-repeat: no-repeat;
}

.keep-open-share:after {
    display: none;
}

.keep-wx-button {
    background-color: #1aad19;
    font-size: 36rpx;
    color: #fff;
}

.keep-wx-button .icon {
    display: inline-block;
    vertical-align: middle;
    margin-right: 10rpx;
    width: 50rpx;
    height: 41rpx;
    background-image: url("data:img/jpg;base64,");
    background-size: cover;
}

.keep-wx-button:after {
    display: none;
}

.keep-border-btn {
    position: relative;
}

.keep-border-btn:after {
    content: '';
    position: absolute;
    border: 2rpx solid #efefef;
    top: -50%;
    left: -50%;
    right: -50%;
    bottom: -50%;
    transform: scale(0.5);
    border-radius: 8rpx;
}

.keep-border-btn.green:after {
    border-color: #24c789;
}

.keep-vip-icon {
    margin-left: 12rpx;
    width: 32rpx;
    height: 32rpx;
}

.iphonex {
    padding-bottom: 48rpx;
}

.keep-month-demon-title-new {
    position: relative;
    margin: 0;
    padding: 0 16rpx 0 32rpx;
    height: 36rpx;
    line-height: 36rpx;
    border-radius: 0 36rpx 36rpx 0;
    background-image: linear-gradient(90deg,#3e3e3e 0%,#2b2b2b 100%);
    font-size: 20rpx;
    color: #fff;
}

.keep-month-demon-title-new:before {
    position: absolute;
    top: -6rpx;
    left: -22rpx;
    content: '';
    width: 44rpx;
    height: 48rpx;
    background-image: url("https://static1.keepcdn.com/2019/09/30/15/1569827518874_66x72.png");
    background-size: 44rpx 48rpx;
    background-repeat: no-repeat;
}

.keep-month-demon-title {
    position: relative;
    display: inline-block;
    padding: 0 12rpx 0 37rpx;
    margin-left: 45rpx;
    height: 42rpx;
    line-height: 42rpx;
    border-radius: 0 42rpx 42rpx 0;
    background-image: linear-gradient(90deg,#3e3e3e 7%,#2b2b2b 100%);
    font-size: 20rpx;
    font-weight: bold;
    color: #fff;
}

.keep-month-demon-title:first-child {
    margin-left: 25rpx;
}

.keep-month-demon-title:before {
    position: absolute;
    top: -4rpx;
    left: -25rpx;
    content: '';
    width: 50rpx;
    height: 50rpx;
    background-image: url("https://static1.keepcdn.com/2019/01/09/15/1547019058522_50x50.png");
    background-size: 50rpx 50rpx;
    background-repeat: no-repeat;
}

.keep-month-demon-horn-small {
    position: relative;
}

.keep-month-demon-horn-small:before {
    position: absolute;
    top: 0;
    left: 0;
    content: '';
    width: 100%;
    height: 100%;
    background-image: url("https://static1.keepcdn.com/2019/01/09/18/1547029581015_72x26.png");
    background-size: 72rpx 26rpx;
    background-position: top center;
    background-repeat: no-repeat;
}

.keep-month-demon-horn-big {
    position: relative;
}

.keep-month-demon-horn-big:before {
    position: absolute;
    top: 0;
    left: 0;
    content: '';
    width: 100%;
    height: 100%;
    background-image: url("https://static1.keepcdn.com/2019/01/09/18/1547029599472_108x36.png");
    background-size: 108rpx 36rpx;
    background-position: top center;
    background-repeat: no-repeat;
}

.keep-month-demon-horn-big-new {
    position: relative;
}

.keep-month-demon-horn-big-new:before {
    content: '';
    position: absolute;
    top: 50%;
    left: 50%;
    width: 170rpx;
    height: 170rpx;
    transform: translate(-50%,-50%);
    background-image: url("https://static1.keepcdn.com/2019/09/30/15/1569827566613_255x255.png");
    background-repeat: no-repeat;
    background-size: cover;
}

.slogan {
    text-align: center;
    padding-bottom: 30rpx;
    margin-top: 30rpx;
    font-size: 26rpx;
    color: #ccc;
}

.slogan image {
    width: 48rpx;
    height: 48rpx;
}

.medal-mask {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0,0,0,0.7);
    z-index: 20;
    opacity: 0;
    animation: fadeIn 0.3s 0.3s forwards;
}

.medal-box {
    position: fixed;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    width: 580rpx;
    height: 580rpx;
    background-color: #fff;
    border-radius: 8rpx;
    text-align: center;
}

.medal-box .top-mask {
    height: 250rpx;
    position: absolute;
    overflow: hidden;
    top: 0;
    left: 0;
    right: 0;
    border-radius: 8rpx;
}

.medal-box .top-mask:after {
    content: "";
    position: absolute;
    top: -40%;
    left: 0;
    right: 0;
    height: 100%;
    background-image: linear-gradient(to bottom,#24c789 46%,#4aa5d0 100%);
    border-radius: 50%;
    transform: scale(1.8);
}

.medal-box .medal-close {
    display: block;
    height: 48rpx;
    background: url("https://static1.keepcdn.com/2018/08/01/11/1533095496576_64x64.png") no-repeat center;
    background-size: 48rpx;
    position: absolute;
    left: 0;
    right: 0;
    bottom: -112rpx;
}

.medal-box .medal {
    position: relative;
    z-index: 2;
}

.medal-box .medal .medal-title {
    font-size: 28rpx;
    line-height: 40rpx;
    padding-top: 48rpx;
    color: #fff;
}

.medal-box .medal .medal-text {
    font-size: 46rpx;
    line-height: 60rpx;
    padding-top: 4rpx;
    color: #fff;
    font-weight: bold;
}

.medal-box .medal .medal-text .view {
    font-size: 28rpx;
    font-weight: normal;
}

.medal-box .medal .first-pic {
    display: inline-block;
    width: 240rpx;
    height: 150rpx;
    padding-top: 100rpx;
}

.medal-box .medal .medal-pic {
    background: url("https://static1.keepcdn.com/2018/08/28/19/1535454646516_540x540.png") no-repeat center;
    background-size: 300rpx 300rpx;
    padding-top: 300rpx;
    position: relative;
    margin: -90rpx auto 0;
}

.medal-box .medal .medal-pic .image {
    position: absolute;
    top: 108rpx;
    left: 50%;
    transform: translateX(-50%);
    width: 180rpx;
    height: 180rpx;
    border-radius: 50%;
}

.medal-box .medal .medal-pic .medal-gym {
    color: #333;
    font-size: 36rpx;
    padding-top: 10rpx;
}

.medal-box .medal .medal-pic .medal-gymtext {
    display: block;
    font-size: 24rpx;
    color: #999;
    line-height: 33rpx;
    padding-top: 6rpx;
}

.medal-box .medal .medal-tips {
    font-size: 26rpx;
    padding-top: 36rpx;
    color: #999;
}