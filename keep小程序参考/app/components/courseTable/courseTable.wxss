@font-face {
    font-family: 'Keep';
    src: url("https://static1.keepcdn.com/lego/webfont/keepfont.ttf") format('truetype');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family:'Keepfont2019';src:url("https://static1.keepcdn.com/lego/webfont/Keepfont2019.ttf") format('truetype');font-weight:normal;font-style:normal;
}

.keep-right-more-arrow {
    background-image: url("data:img/jpg;base64,iVBORw0KGgoAAAANSUhEUgAAABEAAAAeBAMAAAA4FegcAAAAFVBMVEUAAACloqmloaimprOloamloamkoKjRHZCjAAAABnRSTlMAodEUgoBCKx0OAAAAJ0lEQVQY02NAAEYBGMstEcYySoMJMqvRTxDBQsjSRYghLBEjNBAAAEvtD9kgyY+TAAAAAElFTkSuQmCC");
    background-repeat: no-repeat;
    background-size: 17rpx 30rpx;
    background-position: center right;
}

.keep-right-more-arrow-new {
    background-image: url("https://static1.keepcdn.com/2019/09/30/15/1569827395455_24x51.png");
    background-repeat: no-repeat;
    background-size: 16rpx 34rpx;
    background-position: center right;
}

.keep-location-icon-black {
    background-image: url("data:img/jpg;base64,");
    background-size: 32rpx 32rpx;
    background-position: center left;
    background-repeat: no-repeat;
}

.keep-clock-icon-black {
    background-image: url("data:img/jpg;base64,");
    background-size: 32rpx 32rpx;
    background-position: center left;
    background-repeat: no-repeat;
}

.keep-1px-border {
    position: relative;
}

.keep-1px-border:after {
    position: absolute;
    left: 0;
    right: 0;
    content: '';
    height: 2rpx;
    transform: scaleY(0.5);
    background-color: #efefef;
}

.keep-1px-border.bottom-border:after {
    bottom: 0;
    transform-origin: 0% 100%;
}

.keep-1px-border.top-border:after {
    top: 0;
    transform-origin: 0% 0%;
}

.keep-text-overflow {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.keep-avatar-placeholder {
    background-image: url("data:img/jpg;base64,");
    background-size: cover;
    background-repeat: no-repeat;
}

.keep-course-cover-placeholder {
    background-color: #efefef;
}

.keep-course-cover-mark {
    position: relative;
}

.keep-course-cover-mark:after {
    content: '';
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background-image: linear-gradient(0deg,rgba(88,79,96,0.8) 0,rgba(88,79,96,0) 100%);
}

.keep-star {
    width: 28rpx;
    height: 28rpx;
    background-repeat: no-repeat;
    background-size: cover;
}

.keep-star.all-star {
    background-image: url("https://static1.keepcdn.com/2019/09/30/15/1569827026382_42x42.png");
}

.keep-star.half-star {
    background-image: url("https://static1.keepcdn.com/2019/09/30/16/1569832043771_42x42.png");
}

.keep-star.empty-star {
    background-image: url("https://static1.keepcdn.com/2019/09/30/16/1569831954695_42x42.png");
}

.keep-open-share {
    display: block;
    margin-top: 12rpx;
    margin-left: 47rpx;
    width: 46rpx;
    height: 46rpx;
    background-color: #fff;
    border-radius: 0;
    background-image: url("data:img/jpg;base64,");
    background-size: 46rpx 46rpx;
    background-repeat: no-repeat;
}

.keep-open-share:after {
    display: none;
}

.keep-wx-button {
    background-color: #1aad19;
    font-size: 36rpx;
    color: #fff;
}

.keep-wx-button .icon {
    display: inline-block;
    vertical-align: middle;
    margin-right: 10rpx;
    width: 50rpx;
    height: 41rpx;
    background-image: url("data:img/jpg;base64,");
    background-size: cover;
}

.keep-wx-button:after {
    display: none;
}

.keep-border-btn {
    position: relative;
}

.keep-border-btn:after {
    content: '';
    position: absolute;
    border: 2rpx solid #efefef;
    top: -50%;
    left: -50%;
    right: -50%;
    bottom: -50%;
    transform: scale(0.5);
    border-radius: 8rpx;
}

.keep-border-btn.green:after {
    border-color: #24c789;
}

.keep-vip-icon {
    margin-left: 12rpx;
    width: 32rpx;
    height: 32rpx;
}

.iphonex {
    padding-bottom: 48rpx;
}

.keep-month-demon-title-new {
    position: relative;
    margin: 0;
    padding: 0 16rpx 0 32rpx;
    height: 36rpx;
    line-height: 36rpx;
    border-radius: 0 36rpx 36rpx 0;
    background-image: linear-gradient(90deg,#3e3e3e 0%,#2b2b2b 100%);
    font-size: 20rpx;
    color: #fff;
}

.keep-month-demon-title-new:before {
    position: absolute;
    top: -6rpx;
    left: -22rpx;
    content: '';
    width: 44rpx;
    height: 48rpx;
    background-image: url("https://static1.keepcdn.com/2019/09/30/15/1569827518874_66x72.png");
    background-size: 44rpx 48rpx;
    background-repeat: no-repeat;
}

.keep-month-demon-title {
    position: relative;
    display: inline-block;
    padding: 0 12rpx 0 37rpx;
    margin-left: 45rpx;
    height: 42rpx;
    line-height: 42rpx;
    border-radius: 0 42rpx 42rpx 0;
    background-image: linear-gradient(90deg,#3e3e3e 7%,#2b2b2b 100%);
    font-size: 20rpx;
    font-weight: bold;
    color: #fff;
}

.keep-month-demon-title:first-child {
    margin-left: 25rpx;
}

.keep-month-demon-title:before {
    position: absolute;
    top: -4rpx;
    left: -25rpx;
    content: '';
    width: 50rpx;
    height: 50rpx;
    background-image: url("https://static1.keepcdn.com/2019/01/09/15/1547019058522_50x50.png");
    background-size: 50rpx 50rpx;
    background-repeat: no-repeat;
}

.keep-month-demon-horn-small {
    position: relative;
}

.keep-month-demon-horn-small:before {
    position: absolute;
    top: 0;
    left: 0;
    content: '';
    width: 100%;
    height: 100%;
    background-image: url("https://static1.keepcdn.com/2019/01/09/18/1547029581015_72x26.png");
    background-size: 72rpx 26rpx;
    background-position: top center;
    background-repeat: no-repeat;
}

.keep-month-demon-horn-big {
    position: relative;
}

.keep-month-demon-horn-big:before {
    position: absolute;
    top: 0;
    left: 0;
    content: '';
    width: 100%;
    height: 100%;
    background-image: url("https://static1.keepcdn.com/2019/01/09/18/1547029599472_108x36.png");
    background-size: 108rpx 36rpx;
    background-position: top center;
    background-repeat: no-repeat;
}

.keep-month-demon-horn-big-new {
    position: relative;
}

.keep-month-demon-horn-big-new:before {
    content: '';
    position: absolute;
    top: 50%;
    left: 50%;
    width: 170rpx;
    height: 170rpx;
    transform: translate(-50%,-50%);
    background-image: url("https://static1.keepcdn.com/2019/09/30/15/1569827566613_255x255.png");
    background-repeat: no-repeat;
    background-size: cover;
}

.slogan {
    text-align: center;
    padding-bottom: 30rpx;
    margin-top: 30rpx;
    font-size: 26rpx;
    color: #ccc;
}

.slogan image {
    width: 48rpx;
    height: 48rpx;
}

.course-table {
    font-size: 28rpx;
    background-color: #fafafa;
}

.course-table .flex {
    display: flex;
    align-items: center;
}

.course-table .flex-between {
    justify-content: space-between;
}

.course-table .mid-button {
    width: 240rpx;
    height: 72rpx;
    line-height: 72rpx;
    font-size: 28rpx;
    margin-top: 28rpx;
    border: 0;
}

.course-table .coach {
    width: 130rpx;
    height: 130rpx;
}

.course-table .table {
    margin-bottom: 20rpx;
    padding-top: 20rpx;
}

.course-table .table:first-child {
    padding-top: 0;
}

.course-table .table-head {
    display: flex;
    align-items: center;
    justify-content: space-between;
    height: 130rpx;
    margin: 0 30rpx;
}

.course-table .table-head .head-wrap {
    display: flex;
    align-items: center;
    white-space: nowrap;
    font-size: 0;
}

.course-table .table-head .head-wrap .gym-name {
    font-size: 36rpx;
    color: #000;
    font-weight: bold;
    padding-right: 16rpx;
}

.course-table .table-head .tips {
    padding-right: 35rpx;
    font-size: 26rpx;
    color: rgba(0,0,0,0.4);
}

.course-table .building,.course-table .beginning {
    height: 65vh;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    background: #fff;
    box-sizing: border-box;
}

.course-table .building image,.course-table .beginning image {
    width: 380rpx;
    height: 220rpx;
    display: block;
    margin-bottom: 32rpx;
}

.course-table .building image.icon-schedule,.course-table .beginning image.icon-schedule {
    width: 300rpx;
    height: 200rpx;
}

.course-table .building .tip1,.course-table .beginning .tip1 {
    color: rgba(0,0,0,0.7);
    font-size: 28rpx;
    line-height: 40rpx;
}

.course-table .building .tip2,.course-table .beginning .tip2 {
    font-size: 24rpx;
    color: rgba(0,0,0,0.4);
    line-height: 34rpx;
    padding-top: 16rpx;
}

.course-table .table-cell {
    position: relative;
    margin: 0 30rpx 20rpx;
    padding-top: 30rpx;
}

.course-table .table-cell .corner {
    position: absolute;
    top: 10rpx;
    right: 10rpx;
    display: block;
    width: 66rpx;
    height: 66rpx;
}

.course-table .table-cell .time {
    width: 230rpx;
    padding-left: 20rpx;
    line-height: 46rpx;
    font-size: 32rpx;
    font-weight: bold;
    color: #24c789;
    background-image: linear-gradient(270deg,rgba(0,0,0,0) 0%,rgba(0,0,0,0.08) 100%);
}

.course-table .table-cell .introduce {
    box-sizing: border-box;
    display: flex;
    align-items: center;
    padding: 0 20rpx;
    position: relative;
}

.course-table .table-cell .introduce .to-course {
    flex: 1;
    display: inline-flex;
    align-items: center;
    justify-content: space-between;
    width: 100%;
}

.course-table .table-cell .introduce .coach-avatar {
    display: block;
    width: 130rpx;
    height: 130rpx;
    margin-right: 22rpx;
    border-radius: 50%;
}

.course-table .table-cell .introduce .course-info {
    width: 100%;
    white-space: nowrap;
    padding: 30rpx 0 30rpx 20rpx;
    box-sizing: border-box;
}

.course-table .table-cell .introduce .course-info .name {
    font-size: 30rpx;
    line-height: 42rpx;
    color: #000;
    font-weight: bold;
    letter-spacing: -0.3px;
    overflow: hidden;
    text-overflow: ellipsis;
    width: 95%;
}

.course-table .table-cell .introduce .course-info .tags {
    margin-top: 8rpx;
    font-size: 22rpx;
    line-height: 30rpx;
    color: rgba(0,0,0,0.4);
}

.course-table .table-cell .introduce .course-info .course-tag {
    padding-bottom: 8rpx;
    font-size: 0;
}

.course-table .table-cell .introduce .course-info .price-info {
    height: 50rpx;
    margin-top: 34rpx;
    line-height: 50rpx;
    display: flex;
    align-items: center;
}

.course-table .table-cell .introduce .course-info .price-info .sale {
    font-size: 26rpx;
    font-weight: bold;
    color: #24c789;
}

.course-table .table-cell .introduce .course-info .price-info .sale text {
    font-size: 36rpx;
    padding-left: 2rpx;
}

.course-table .table-cell .introduce .course-info .price-info .origin {
    margin-left: 16rpx;
    text-decoration: line-through;
    font-size: 26rpx;
    color: rgba(0,0,0,0.2);
    margin-top: 2rpx;
}

.course-table .table-cell .introduce .course-info .price-info .package-amount {
    margin-top: 2rpx;
    margin-left: 15rpx;
    padding-left: 15rpx;
    line-height: 20rpx;
    border-left: 1rpx solid #ccc;
    font-size: 24rpx;
    color: rgba(0,0,0,0.7);
}

.course-table .table-cell .introduce .course-info .price-info .package-amount .amount {
    color: #24c789;
    font-weight: bold;
}

.course-table .table-cell .introduce .btn-wrap {
    position: absolute;
    bottom: 40rpx;
    right: 20rpx;
}

.course-table .table-cell .introduce .course-book-count,.course-table .table-cell .introduce .waiting-success-rate {
    font-size: 20rpx;
    color: rgba(0,0,0,0.4);
    line-height: 1;
    padding-top: 12rpx;
    text-align: center;
}

.course-table .table-cell .introduce .waiting-success-rate {
    color: #24c789;
    font-weight: bold;
}

.course-table .table-cell .activity-tags {
    border-top: 1rpx solid #efefef;
    margin: 0 20rpx;
    padding: 20rpx 0;
}

.course-table .table-cell .activity-tags .tag-item {
    display: flex;
    height: 58rpx;
    align-items: center;
    font-size: 0;
}

.course-table .table-cell .activity-tags .tag-item .tag-text {
    font-size: 24rpx;
    padding-left: 16rpx;
    color: rgba(0,0,0,0.4);
}

.course-table .table-empty {
    height: 224rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    margin: 0 30rpx;
    padding-left: 170rpx;
    background: #fff url("https://static1.keepcdn.com/2018/07/12/10/1531361733516_300x200.png") no-repeat center left 20rpx;
    background-size: 150rpx 100rpx;
    position: relative;
}

.course-table .table-empty .tip1 {
    color: rgba(0,0,0,0.7);
    font-size: 30rpx;
    line-height: 42rpx;
}

.course-table .table-empty .tip2 {
    font-size: 24rpx;
    color: rgba(0,0,0,0.4);
    line-height: 34rpx;
    padding-top: 16rpx;
}

.course-table .table-empty .buy-classbag {
    position: absolute;
    right: 20rpx;
    top: 50%;
    transform: translateY(-50%);
}

.add-animation-wrap {
    position: fixed;
    top: 0;
    right: 130rpx;
    z-index: 99;
    margin: -20rpx 0 0 -20rpx;
    animation: xAxis 1.3s forwards;
}

.add-animation-wrap .add-animation {
    width: 40rpx;
    height: 40rpx;
    border-radius: 50%;
    background: rgba(36,199,137,0.9);
    animation: yAxis 1.3s forwards cubic-bezier(0.3,0.3,0.1,1.6);
}

.keep-right-more-arrow {
    background-image: url("data:img/jpg;base64,iVBORw0KGgoAAAANSUhEUgAAABEAAAAeBAMAAAA4FegcAAAAFVBMVEUAAACloqmloaimprOloamloamkoKjRHZCjAAAABnRSTlMAodEUgoBCKx0OAAAAJ0lEQVQY02NAAEYBGMstEcYySoMJMqvRTxDBQsjSRYghLBEjNBAAAEvtD9kgyY+TAAAAAElFTkSuQmCC");
    background-repeat: no-repeat;
    background-size: 17rpx 30rpx;
    background-position: center right;
}