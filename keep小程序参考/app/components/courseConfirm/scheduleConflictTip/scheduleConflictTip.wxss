.keep-right-more-arrow-new {
    background-image: url("https://static1.keepcdn.com/2019/09/30/15/1569827395455_24x51.png");
    background-repeat: no-repeat;
    background-size: 16rpx 34rpx;
    background-position: center right;
}

.keep-location-icon-black {
    background-image: url("data:img/jpg;base64,");
    background-size: 32rpx 32rpx;
    background-position: center left;
    background-repeat: no-repeat;
}

.keep-clock-icon-black {
    background-image: url("data:img/jpg;base64,");
    background-size: 32rpx 32rpx;
    background-position: center left;
    background-repeat: no-repeat;
}

.keep-1px-border {
    position: relative;
}

.keep-1px-border:after {
    position: absolute;
    left: 0;
    right: 0;
    content: '';
    height: 2rpx;
    transform: scaleY(0.5);
    background-color: #efefef;
}

.keep-1px-border.bottom-border:after {
    bottom: 0;
    transform-origin: 0% 100%;
}

.keep-1px-border.top-border:after {
    top: 0;
    transform-origin: 0% 0%;
}

.keep-text-overflow {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.keep-avatar-placeholder {
    background-image: url("data:img/jpg;base64,");
    background-size: cover;
    background-repeat: no-repeat;
}

.keep-course-cover-placeholder {
    background-color: #efefef;
}

.keep-course-cover-mark {
    position: relative;
}

.keep-course-cover-mark:after {
    content: '';
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background-image: linear-gradient(0deg,rgba(88,79,96,0.8) 0,rgba(88,79,96,0) 100%);
}

.keep-star {
    width: 28rpx;
    height: 28rpx;
    background-repeat: no-repeat;
    background-size: cover;
}

.keep-star.all-star {
    background-image: url("https://static1.keepcdn.com/2019/09/30/15/1569827026382_42x42.png");
}

.keep-star.half-star {
    background-image: url("https://static1.keepcdn.com/2019/09/30/16/1569832043771_42x42.png");
}

.keep-star.empty-star {
    background-image: url("https://static1.keepcdn.com/2019/09/30/16/1569831954695_42x42.png");
}

.keep-open-share {
    display: block;
    margin-top: 12rpx;
    margin-left: 47rpx;
    width: 46rpx;
    height: 46rpx;
    background-color: #fff;
    border-radius: 0;
    background-image: url("data:img/jpg;base64,");
    background-size: 46rpx 46rpx;
    background-repeat: no-repeat;
}

.keep-open-share:after {
    display: none;
}

.keep-wx-button {
    background-color: #1aad19;
    font-size: 36rpx;
    color: #fff;
}

.keep-wx-button .icon {
    display: inline-block;
    vertical-align: middle;
    margin-right: 10rpx;
    width: 50rpx;
    height: 41rpx;
    background-image: url("data:img/jpg;base64,");
    background-size: cover;
}

.keep-wx-button:after {
    display: none;
}

.keep-border-btn {
    position: relative;
}

.keep-border-btn:after {
    content: '';
    position: absolute;
    border: 2rpx solid #efefef;
    top: -50%;
    left: -50%;
    right: -50%;
    bottom: -50%;
    transform: scale(0.5);
    border-radius: 8rpx;
}

.keep-border-btn.green:after {
    border-color: #24c789;
}

.keep-vip-icon {
    margin-left: 12rpx;
    width: 32rpx;
    height: 32rpx;
}

.iphonex {
    padding-bottom: 48rpx;
}

.keep-month-demon-title-new {
    position: relative;
    margin: 0;
    padding: 0 16rpx 0 32rpx;
    height: 36rpx;
    line-height: 36rpx;
    border-radius: 0 36rpx 36rpx 0;
    background-image: linear-gradient(90deg,#3e3e3e 0%,#2b2b2b 100%);
    font-size: 20rpx;
    color: #fff;
}

.keep-month-demon-title-new:before {
    position: absolute;
    top: -6rpx;
    left: -22rpx;
    content: '';
    width: 44rpx;
    height: 48rpx;
    background-image: url("https://static1.keepcdn.com/2019/09/30/15/1569827518874_66x72.png");
    background-size: 44rpx 48rpx;
    background-repeat: no-repeat;
}

.keep-month-demon-title {
    position: relative;
    display: inline-block;
    padding: 0 12rpx 0 37rpx;
    margin-left: 45rpx;
    height: 42rpx;
    line-height: 42rpx;
    border-radius: 0 42rpx 42rpx 0;
    background-image: linear-gradient(90deg,#3e3e3e 7%,#2b2b2b 100%);
    font-size: 20rpx;
    font-weight: bold;
    color: #fff;
}

.keep-month-demon-title:first-child {
    margin-left: 25rpx;
}

.keep-month-demon-title:before {
    position: absolute;
    top: -4rpx;
    left: -25rpx;
    content: '';
    width: 50rpx;
    height: 50rpx;
    background-image: url("https://static1.keepcdn.com/2019/01/09/15/1547019058522_50x50.png");
    background-size: 50rpx 50rpx;
    background-repeat: no-repeat;
}

.keep-month-demon-horn-small {
    position: relative;
}

.keep-month-demon-horn-small:before {
    position: absolute;
    top: 0;
    left: 0;
    content: '';
    width: 100%;
    height: 100%;
    background-image: url("https://static1.keepcdn.com/2019/01/09/18/1547029581015_72x26.png");
    background-size: 72rpx 26rpx;
    background-position: top center;
    background-repeat: no-repeat;
}

.keep-month-demon-horn-big {
    position: relative;
}

.keep-month-demon-horn-big:before {
    position: absolute;
    top: 0;
    left: 0;
    content: '';
    width: 100%;
    height: 100%;
    background-image: url("https://static1.keepcdn.com/2019/01/09/18/1547029599472_108x36.png");
    background-size: 108rpx 36rpx;
    background-position: top center;
    background-repeat: no-repeat;
}

.keep-month-demon-horn-big-new {
    position: relative;
}

.keep-month-demon-horn-big-new:before {
    content: '';
    position: absolute;
    top: 50%;
    left: 50%;
    width: 170rpx;
    height: 170rpx;
    transform: translate(-50%,-50%);
    background-image: url("https://static1.keepcdn.com/2019/09/30/15/1569827566613_255x255.png");
    background-repeat: no-repeat;
    background-size: cover;
}

.slogan {
    text-align: center;
    padding-bottom: 30rpx;
    margin-top: 30rpx;
    font-size: 26rpx;
    color: #ccc;
}

.slogan image {
    width: 48rpx;
    height: 48rpx;
}

.schedule-conflict-tip {
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    top: 0;
    z-index: 20;
    background-color: rgba(0,0,0,0.5);
    display: flex;
    align-items: center;
    justify-content: center;
}

.body {
    width: 580rpx;
    padding: 48rpx 30rpx 16rpx;
    box-sizing: border-box;
    border-radius: 8rpx;
    background-color: #fff;
}

.body .title {
    font-size: 36rpx;
    color: #333;
    text-align: center;
}

.body .conflict-schedule {
    margin: 38rpx 0 40rpx;
    border: 1rpx solid #efefef;
    box-shadow: 0 4rpx 16rpx 0 rgba(0,0,0,0.04);
    border-radius: 8rpx;
    overflow: hidden;
}

.body .conflict-schedule .date {
    margin-top: 32rpx;
    display: inline-block;
    height: 44rpx;
    line-height: 44rpx;
    border-radius: 0 44rpx 44rpx 0;
    padding: 0 20rpx;
    background-color: #efefef;
    font-size: 26rpx;
    font-weight: bold;
    color: #333;
}

.body .conflict-schedule .schedule-wrap {
    margin: 28rpx 20rpx 32rpx;
    display: flex;
    align-items: center;
}

.body .conflict-schedule .schedule-wrap .coach {
    display: block;
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
}

.body .conflict-schedule .schedule-wrap .schedule {
    margin-left: 16rpx;
}

.body .conflict-schedule .schedule-wrap .schedule .name {
    width: 360rpx;
    font-size: 26rpx;
    color: #333;
}

.body .conflict-schedule .schedule-wrap .schedule .gym {
    margin-top: 11rpx;
    padding-left: 31rpx;
    font-size: 24rpx;
    color: #999;
    background: url("https://static1.keepcdn.com/2018/11/07/18/1541587721258_32x32.png") no-repeat -6rpx 2rpx;
    background-size: 32rpx;
}

.body .button {
    margin: 0 auto;
    width: 480rpx;
    height: 100rpx;
    line-height: 100rpx;
    border-radius: 100rpx;
    text-align: center;
}

.body .button.confirm {
    background-color: #24c789;
    font-size: 36rpx;
    color: #fff;
}

.body .button.cancel {
    font-size: 28rpx;
    color: #584f60;
}