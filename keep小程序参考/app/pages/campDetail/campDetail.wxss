page {
    background-color: #fff;
}

.hidden {
    height: calc(100vh - 68px);
    overflow: hidden;
}

.banner-wrap {
    position: relative;
}

.banner-wrap .banner-tip {
    position: absolute;
    bottom: 60rpx;
    left: 50%;
    transform: translateX(-50%);
    width: 180rpx;
    height: 42rpx;
    border-radius: 42rpx;
    background: rgba(255,255,255,0.4);
    font-size: 22rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.banner-wrap .banner-tip:after {
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    width: 50%;
    height: 100%;
    background-image: linear-gradient(-45deg,#4aa5d0 0%,#24c789 100%);
    border-radius: 42rpx;
    transition: all 0.3s;
}

.banner-wrap .banner-tip view {
    flex: 1;
    text-align: center;
    position: relative;
    z-index: 3;
    transition: color 0.3s;
}

.banner-wrap .banner-tip view:nth-child(1) {
    color: #fff;
}

.banner-wrap .banner-tip view:nth-child(2) {
    color: #000;
}

.banner-wrap .banner-tip.on view:nth-child(1) {
    color: #000;
}

.banner-wrap .banner-tip.on view:nth-child(2) {
    color: #fff;
}

.banner-wrap .banner-tip.on:after {
    transform: translateX(100%);
}

.banner-wrap .banner-count {
    position: absolute;
    bottom: 60rpx;
    right: 30rpx;
    background: rgba(0,0,0,0.4);
    border-radius: 4rpx;
    padding: 0 10rpx;
    height: 38rpx;
    line-height: 38rpx;
    color: #fff;
    font-size: 24rpx;
    font-weight: bold;
}

.banner-wrap .banner-mask {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    padding-bottom: 20rpx;
    background-image: linear-gradient(180deg,rgba(0,0,0,0.5) 0%,rgba(0,0,0,0) 100%);
}

.banner-wrap .banner {
    height: 500rpx;
    background-color: #efefef;
}

.banner-wrap .banner swiper-item {
    position: relative;
}

.banner-wrap .banner swiper-item image {
    width: 100%;
    height: 100%;
}

.banner-wrap .banner swiper-item .play-icon {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    width: 112rpx;
    height: 112rpx;
    background: url("https://static1.keepcdn.com/2019/03/04/18/1551694779217_112x112.png") no-repeat center;
    background-size: cover;
}

.video-wrap {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 20;
    background: rgba(0,0,0,0.5);
    animation: fadeIn 0.4s forwards;
}

.video-wrap video {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    transform: translateY(100%);
}

.video-wrap video.autoplay {
    animation: slideIn 0.5s forwards;
}

.video-wrap cover-view {
    position: fixed;
    top: 0;
    left: 0;
    width: 48rpx;
    height: 48rpx;
    padding: 28rpx;
    z-index: 2;
}

.video-wrap cover-image {
    width: 48rpx;
    height: 48rpx;
}

.base-wrap {
    position: relative;
    margin: -30rpx 30rpx 0;
}

.base-wrap .base {
    padding: 0 30rpx 0;
}

.base-wrap .base .countdown-wrap {
    height: 68rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 18rpx 30rpx;
    margin: 0 -30rpx;
    border-radius: 8rpx 8rpx 0 0;
    box-sizing: border-box;
}

.base-wrap .base .countdown-wrap .sign-info {
    color: #fff;
    font-weight: bold;
    font-size: 30rpx;
    line-height: 42rpx;
}

.base-wrap .base .countdown-wrap .countdown {
    display: flex;
    align-items: center;
}

.base-wrap .base .countdown-wrap .countdown .text {
    margin: 8rpx;
    font-size: 24rpx;
    font-weight: bold;
    color: #fff;
}

.base-wrap .base .countdown-wrap .countdown .text:last-of-type {
    margin-right: 0;
}

.base-wrap .base .countdown-wrap .countdown .num {
    background-color: #fff;
    border-radius: 4rpx;
    width: 36rpx;
    height: 32rpx;
    line-height: 32rpx;
    text-align: center;
    font-family: Keepfont2019;
    font-size: 26rpx;
    color: #ff5363;
}

.base-wrap .base .going {
    background: url("https://static1.keepcdn.com/2019/11/27/12/1574828619009_690x68.png") no-repeat left;
    background-size: cover;
}

.base-wrap .base .over {
    background: url("https://static1.keepcdn.com/2019/11/27/12/1574828353422_690x68.png") no-repeat left;
    background-size: cover;
}

.base-wrap .base .name {
    line-height: 60rpx;
    font-size: 46rpx;
    color: #000;
    font-weight: bold;
    margin-top: 30rpx;
}

.base-wrap .base .course-num {
    margin: 16rpx 0rpx 32rpx;
    font-size: 30rpx;
    color: rgba(0,0,0,0.7);
    line-height: 34rpx;
}

.base-wrap .base .tags-book-count {
    margin-top: 30rpx;
    padding-bottom: 40rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    flex-wrap: wrap;
}

.base-wrap .base .tags-book-count .tags-wrap {
    font-size: 0;
    white-space: nowrap;
}

.base-wrap .base .tags-book-count .tags-wrap .tags {
    margin-right: 20rpx;
}

.base-wrap .base .camp-info {
    display: flex;
    justify-content: space-between;
}

.base-wrap .base .camp-info .info {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    height: 178rpx;
}

.base-wrap .base .camp-info .info.camp-course-num {
    align-items: flex-start;
}

.base-wrap .base .camp-info .info.camp-difficulty {
    align-items: flex-end;
}

.base-wrap .base .camp-info .info .title {
    line-height: 34rpx;
    font-size: 24rpx;
    color: rgba(0,0,0,0.4);
}

.base-wrap .base .camp-info .info .count {
    margin-top: 18rpx;
    line-height: 44rpx;
    font-family: 'Keepfont2019';
    font-size: 50rpx;
    color: #000;
    background-color: #fff;
}

.base-wrap .base .camp-info .info .count.difficulty {
    padding-left: 34rpx;
    color: #24c789;
    background-image: url("data:img/jpg;base64,");
    background-repeat: no-repeat;
    background-size: 32rpx 42rpx;
}

.gym-card-wrap {
    margin: 20rpx 30rpx 0;
    padding: 0 30rpx;
}

.gym-card-wrap .capacity-gym {
    padding: 40rpx 0;
}

.gym-card-wrap .capacity-gym .capacity,.gym-card-wrap .capacity-gym .gym {
    line-height: 36rpx;
    padding-left: 42rpx;
    background: no-repeat center left;
    background-size: 32rpx 32rpx;
    font-size: 26rpx;
    color: rgba(0,0,0,0.4);
}

.gym-card-wrap .capacity-gym .capacity {
    background-image: url("https://static1.keepcdn.com/2019/11/22/17/1574416216902_32x32.png");
}

.gym-card-wrap .capacity-gym .gym {
    margin-top: 20rpx;
    background-image: url("https://static1.keepcdn.com/2019/10/06/18/1570358292846_48x48.png");
}

.gym-card-wrap .coach-wrap {
    padding: 40rpx 0;
}

.gym-card-wrap .coach-wrap .coach {
    display: flex;
    align-items: center;
    height: 90rpx;
    margin-bottom: 20rpx;
}

.gym-card-wrap .coach-wrap .coach:last-child {
    margin-bottom: 0;
}

.gym-card-wrap .coach-wrap .coach .coach-name {
    flex: 1;
    margin-left: 20rpx;
    line-height: 42rpx;
    font-size: 30rpx;
    color: rgba(0,0,0,0.7);
}

.gym-card-wrap .coach-desc {
    margin-top: 30rpx;
    font-size: 26rpx;
    color: rgba(0,0,0,0.7);
    line-height: 40rpx;
}

.gym-card-wrap .book-user {
    padding: 40rpx 0;
}

.gym-card-wrap .book-user>navigator {
    position: relative;
    height: 70rpx;
}

.gym-card-wrap .book-user>navigator .avatar {
    position: absolute;
    top: 0;
    display: block;
    width: 70rpx;
    height: 70rpx;
}

.gym-card-wrap .book-user>navigator .book-count {
    position: absolute;
    right: 36rpx;
    top: 18rpx;
    font-size: 26rpx;
    color: rgba(0,0,0,0.4);
    line-height: 36rpx;
}

.gym-card-wrap .book-user>navigator .book-count .book {
    margin-left: 10rpx;
    color: #24c789;
    font-weight: bold;
}

.gym-card-wrap .book-user>navigator .book-count .total {
    color: #000;
    font-weight: bold;
}

.camp-wrap {
    padding: 40rpx 30rpx 196rpx;
}

.camp-wrap .common-title {
    display: flex;
    align-items: center;
    justify-content: space-between;
    height: 110rpx;
    font-size: 36rpx;
    font-weight: bold;
    color: #000;
}

.camp-wrap .common-title .logo {
    display: block;
    width: 200rpx;
    height: 110rpx;
}

.camp-wrap .arrange-wrap {
    margin-top: 41rpx;
}

.camp-wrap .arrange-wrap .arrange-title {
    justify-content: flex-start;
}

.camp-wrap .arrange-wrap .beats-num {
    font-size: 32rpx;
    font-weight: bold;
    color: rgba(0,0,0,0.5);
}

.camp-wrap .arrange-wrap .arrange {
    background: #fafafa;
    border-radius: 8rpx;
    margin-bottom: 30rpx;
}

.camp-wrap .arrange-wrap .arrange .period-name {
    height: 50rpx;
    width: 100%;
    background-image: linear-gradient(270deg,rgba(0,0,0,0) 0%,rgba(0,0,0,0.08) 100%);
    border-radius: 8rpx 8rpx 0 0;
    padding-left: 30rpx;
    font-size: 32rpx;
    font-weight: bold;
    color: #24c789;
    line-height: 50rpx;
}

.camp-wrap .arrange-wrap .arrange .camp-course {
    display: flex;
    padding: 26rpx 30rpx;
    align-items: flex-start;
}

.camp-wrap .arrange-wrap .arrange .camp-course .camp-course-serial {
    font-family: Keepfont2019;
    font-size: 40rpx;
    color: rgba(0,0,0,0.2);
    margin-right: 24rpx;
}

.camp-wrap .arrange-wrap .arrange .camp-course .camp-course-info {
    display: flex;
    flex-direction: column;
}

.camp-wrap .arrange-wrap .arrange .camp-course .camp-course-info .camp-course-name {
    font-size: 30rpx;
    color: rgba(0,0,0,0.7);
    margin-bottom: 8rpx;
}

.camp-wrap .arrange-wrap .arrange .camp-course .camp-course-info .camp-course-time {
    font-size: 24rpx;
    color: rgba(0,0,0,0.5);
}

.camp-wrap .brand-desc {
    line-height: 40rpx;
    font-size: 26rpx;
    color: rgba(0,0,0,0.7);
}

.camp-wrap .camp-desc {
    margin-top: 30rpx;
    padding: 50rpx 30rpx 30rpx;
    line-height: 40rpx;
    border-radius: 8rpx;
    font-size: 26rpx;
    color: rgba(0,0,0,0.4);
    text-indent: 3em;
    background: url("https://static1.keepcdn.com/2019/03/05/16/1551775006496_52x46.png") no-repeat 30rpx 33rpx;
    background-size: 52rpx 46rpx;
    background-color: #fafafa;
}

.camp-wrap .camp-desc view {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 5;
    -webkit-box-orient: vertical;
    word-break: break-all;
}

.camp-wrap .suited {
    margin-top: 40rpx;
}

.camp-wrap .suited>view {
    padding-bottom: 40rpx;
}

.camp-wrap .suited>view:last-child {
    padding-bottom: 0;
}

.camp-wrap .suited .desc {
    font-size: 26rpx;
    color: rgba(0,0,0,0.7);
    line-height: 40rpx;
}

.btn {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: 8;
    box-shadow: 0 -10rpx 30rpx 0 rgba(0,0,0,0.1);
    display: flex;
}

.btn.divide-7-5 .count {
    flex: 7;
}

.btn.divide-7-5 .count.iphonex {
    padding-bottom: 48rpx;
}

.btn.divide-7-5 .submit {
    flex: 5;
}

.btn keep-button .iphonex {
    box-sizing: content-box;
    padding-bottom: 48rpx;
}

.btn .count {
    flex: 1;
    font-size: 24rpx;
    color: rgba(0,0,0,0.7);
    background-color: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
}

.btn .count .bold {
    font-weight: bold;
}

.btn .count.schedule-count .sales-price {
    font-size: 46rpx;
    color: #24c789;
    font-weight: bold;
}

.btn .count.schedule-count .sales-price .unit {
    position: relative;
    top: -2rpx;
    margin-right: 2rpx;
    font-size: 26rpx;
}

.btn .count.schedule-count .sales-price .pay-limitation {
    font-size: 24rpx;
    font-weight: normal;
    color: rgba(0,0,0,0.7);
    text-align: left;
    line-height: 34rpx;
}

.btn .count.schedule-count .price {
    margin: 8rpx 0 0 16rpx;
    line-height: 38rpx;
    font-size: 26rpx;
    color: rgba(0,0,0,0.2);
    text-decoration: line-through;
}

.btn .count.schedule-count .package-price {
    position: relative;
    margin: 14rpx 0 0 16rpx;
    padding-left: 16rpx;
    line-height: 34rpx;
    font-size: 24rpx;
    color: rgba(0,0,0,0.7);
}

.btn .count.schedule-count .package-price .num {
    color: #24c789;
    font-weight: bold;
}

.btn .count.schedule-count .package-price:before {
    content: '';
    position: absolute;
    top: 7rpx;
    left: 0;
    width: 1rpx;
    height: 20rpx;
    background-color: #cfcfcf;
}

.btn .submit {
    flex: 1;
}

.btn .submit .disable-btn {
    opacity: 0.5;
}