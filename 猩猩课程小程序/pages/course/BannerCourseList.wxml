<navigator-bar backgroundColor="#242424" bind_init="_initComponent">
    <tooltip-back-home bind_init="_initComponent" isShow="{{isShowTooltipBackHome}}"></tooltip-back-home>
</navigator-bar>
<sticky-wrapper bind_init="_initComponent" class="stickyCourseNav" height="100rpx" id="stickyCourseNav" top="{{courseSelectedNavTop}}">
    <course-selected-navbar bind_init="_initComponent" bindswitchCourse="_proxy" bindswitchFilter="_proxy" class="course-selected-navbar" courses="{{courses}}" data-wpy-evt="838-0" isFilterActive="{{courseNavMap[selectedValue].isFilterActive}}" isFilterBtn="{{true}}" selectedValue="{{selectedValue}}" slot="sticky"></course-selected-navbar>
    <view class="course-content" slot="content">
        <class-content-filter bind_init="_initComponent" bindconfirmFilter="_proxy" binditemClickFilter="_proxy" bindonFilter="_proxy" bindopenFilter="_proxy" class="class {{[selectedValue==='class'?'class-active':'']}}" data-region-id-se="classListWithFilter" data-region-id-sm="openFilterClassListWithFilter" data-wpy-evt="838-2" filterInfo="{{filterInfo}}" id="class" isBanner="{{true}}"></class-content-filter>
    </view>
</sticky-wrapper>
<page-loading bind_init="_initComponent" bindreload="_proxy" data-wpy-evt="838-6" id="__LoadingPage__"></page-loading>
<show-toast bind_init="_initComponent" id="__Toast__"></show-toast>
<show-modal bind_init="_initComponent" id="__Modal__"></show-modal>
<show-action-sheet bind_init="_initComponent" id="__ActionSheet__"></show-action-sheet>
<show-add-wechat-modal bind_init="_initComponent" id="__ModalAddWeChat__"></show-add-wechat-modal>
<show-picker-view bind_init="_initComponent" id="__PickerView__"></show-picker-view>
<register-user bind_init="_initComponent" id="__ModalRegisterUser__"></register-user>
<unauthorized-msg bind_init="_initComponent" id="__ModalUnauthorizedMsg__"></unauthorized-msg>
<reject-msg bind_init="_initComponent" id="__ModalRejectMsg__"></reject-msg>
<modal-subscribe-msg-guide1 bind_init="_initComponent" id="__ModalSubscribeMsgGuide1__"></modal-subscribe-msg-guide1>
<modal-subscribe-msg-guide2 bind_init="_initComponent" id="__ModalSubscribeMsgGuide2__"></modal-subscribe-msg-guide2>
