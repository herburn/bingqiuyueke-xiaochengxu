<navigator-bar bind_init="_initComponent" class="navigator-bar" style=" {{'background-color:'+navBarBg+';'}}"></navigator-bar>
<view style=" {{'visibility:'+visibility+';'}}">
    <media-area bind_init="_initComponent" class="media-area" posters="{{detail.posterInfo.posters}}" video="{{detail.posterInfo.video}}"></media-area>
    <sticky-wrapper bind_init="_initComponent" bindchangeSticky="_proxy" data-wpy-evt="839-0" height="136rpx" id="stickyPageNav" scrollThreshold="{{pageNavScrollThreshold}}" top="{{pageNavTopPx}}">
        <page-navbar bind_init="_initComponent" bindshare="_proxy" bindswitchPageNav="_proxy" data-wpy-evt="839-1" id="pageNavbar" isShare="{{detail.shareEnable}}" isSticky="{{pageNavIsSticky}}" navActive="{{pageNavActiveIndex}}" navItems="{{navItems}}" slot="sticky"></page-navbar>
        <view class="class-detail-content" slot="content">
            <class-detail-profile-panel bind_init="_initComponent" class="panel profile-panel" profile="{{detail.profile}}"></class-detail-profile-panel>
            <course-introduce-panel bind_init="_initComponent" class="panel" faqs="{{detail.courseIntroduce.faqs}}" gallery="{{detail.courseIntroduce.gallery}}" introduce="{{detail.courseIntroduce.introduce}}" suitablePeople="{{detail.courseIntroduce.suitablePeople}}" tags="{{detail.courseIntroduce.tags}}" thumbnailGallery="{{detail.courseIntroduce.thumbnailGallery}}" title="{{detail.courseIntroduce.title}}" trainingEffect="{{detail.courseIntroduce.trainingEffect}}"></course-introduce-panel>
            <attention-matters-panel bind_init="_initComponent" class="panel" id="attentionPanel" notes="{{detail.attentionMatter.notes}}"></attention-matters-panel>
            <fitness-step-panel bind_init="_initComponent" class="panel"></fitness-step-panel>
        </view>
    </sticky-wrapper>
    <page-resident-button bind_init="_initComponent" bindtapPageBtn="_proxy" class="page-resident-button" data-profile-se="{{detail.profile}}" data-region-id-se="pageBtnClassDetail" data-schedule-id-se="{{scheduleId}}" data-wpy-evt="839-3" id="pageBtn" items="{{pageBtn.items}}" mode="{{pageBtn.mode}}"></page-resident-button>
</view>
<page-loading bind_init="_initComponent" bindreload="_proxy" data-wpy-evt="839-4" id="__LoadingPage__"></page-loading>
<show-toast bind_init="_initComponent" id="__Toast__"></show-toast>
<show-modal bind_init="_initComponent" id="__Modal__"></show-modal>
<show-action-sheet bind_init="_initComponent" id="__ActionSheet__"></show-action-sheet>
<show-add-wechat-modal bind_init="_initComponent" id="__ModalAddWeChat__"></show-add-wechat-modal>
<show-picker-view bind_init="_initComponent" id="__PickerView__"></show-picker-view>
<register-user bind_init="_initComponent" id="__ModalRegisterUser__"></register-user>
<unauthorized-msg bind_init="_initComponent" id="__ModalUnauthorizedMsg__"></unauthorized-msg>
<reject-msg bind_init="_initComponent" id="__ModalRejectMsg__"></reject-msg>
<modal-subscribe-msg-guide1 bind_init="_initComponent" id="__ModalSubscribeMsgGuide1__"></modal-subscribe-msg-guide1>
<modal-subscribe-msg-guide2 bind_init="_initComponent" id="__ModalSubscribeMsgGuide2__"></modal-subscribe-msg-guide2>
<new-user-prompt bind_init="_initComponent" id="__FloatingActionButtonNewUser__"></new-user-prompt>
<alert-after-share bind_init="_initComponent" id="__AlertAfterShare__"></alert-after-share>
