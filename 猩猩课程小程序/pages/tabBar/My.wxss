    color: #767678;
}

.my .panel .vip-box .progress .percent .banana-target {
    -webkit-box-flex: 1;
    flex: 1;
    margin-right: 24rpx;
    margin-left: 30rpx;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    flex-direction: row;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: justify;
    justify-content: space-between;
}

.my .panel .vip-box .progress .percent .banana-target .icon-banana {
    width: 48rpx;
    height: 48rpx;
}

.my .panel .vip-box .progress .percent .target {
    width: 503rpx;
    height: 12rpx;
    margin-right: 24rpx;
    margin-left: 15rpx;
    border-radius: 100rpx;
    background: #242424;
}

.my .panel .vip-box .progress .percent .target .real {
