}

.my .panel .vip-box .equity {
    width: 100%;
    margin-top: 40rpx;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    flex-direction: row;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: justify;
    justify-content: space-between;
    font-size: 28rpx;
    color: #f2f2f7;
}

.my .row {
    position: relative;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: justify;
    justify-content: space-between;
    width: 100%;
    height: 102rpx;
}

.my .row::after {
    position: absolute;
    bottom: 0;
    left: 24rpx;
    z-index: 1;
    width: 678rpx;
    height: 2rpx;
    background-color: rgba(118,118,120,0.1);
    content: '';
}

.my .row .text {
    -webkit-box-flex: 1;
    flex: 1;
    text-align: right;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    flex-direction: row;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: end;
    justify-content: flex-end;
    font-size: 28rpx;
    color: #f2f2f7;
}

.my .row .text .light {
    margin-bottom: -10rpx;
    font-size: 36rpx;
}

.my .row .text.prompt {
    color: #fcc800;
}

.my .row .label {
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    flex-direction: row;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
    margin-left: 24rpx;
    font-size: 28rpx;
    color: #f1f3f5;
}

.my .row .label .circle {
    width: 12rpx;
    height: 12rpx;
    margin-left: 8rpx;
    border-radius: 50%;
    background: #fb6666;
}

.my .row-promotion {
    color: #fff;
}

.my .row-promotion .row-promotion-component {
    width: 100%;
    height: 100%;
}

.my .row-promotion .row-promotion-container {
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    align-items: center;
    height: 100%;
}

.my .row-list .row:last-child::after {
    height: 0;
}

.my .arrow {
    margin-right: 24rpx;
    display: block;
    width: 18rpx;
    height: 18rpx;
    border-bottom: 2rpx solid #c9c9cd;
    border-right: 2rpx solid #c9c9cd;
    transform: rotate(-45deg);
}

.my .arrow-active {
    margin-right: 24rpx;
    display: block;
    width: 18rpx;
    height: 18rpx;
    border-bottom: 2rpx solid #fcc800;
    border-right: 2rpx solid #fcc800;
    transform: rotate(-45deg);
}

.copy-right-bottom {
    display: block;
    margin-top: auto;
    padding: 24rpx 0;
}