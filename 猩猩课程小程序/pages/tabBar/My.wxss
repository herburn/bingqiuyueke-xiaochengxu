page {
    width: 100%;
    height: 100%;
    background-color: #242424;
}

.my {
    width: 100%;
    overflow-x: hidden;
    padding-bottom: 20rpx;
}

.my .card-box {
    position: relative;
    padding: 0 24rpx;
    height: 646rpx;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    flex-direction: row;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
}

.my .card-box .card-bg {
    position: absolute;
    right: 0;
    left: 0;
    z-index: -1;
    width: 100%;
    height: 100%;
}

.my .card-box .card-container {
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: start;
    justify-content: flex-start;
    width: 100%;
    height: 360rpx;
    margin-bottom: 78rpx;
    margin-top: auto;
    border-radius: 16rpx;
    border: 2rpx solid rgba(242,242,247,0.4);
    background: rgba(36,36,36,0.56);
}

.my .card-box .card-container .card-header {
    margin: 32rpx 24rpx 48rpx 24rpx;
    width: 654rpx;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    flex-direction: row;
    -webkit-box-align: start;
    align-items: flex-start;
    -webkit-box-pack: justify;
    justify-content: space-between;
}

.my .card-box .card-container .card-header .super-monkey {
    width: 342rpx;
    height: 42rpx;
}

.my .card-box .card-container .card-center-icon {
    margin: 0 24rpx;
    width: 654rpx;
    height: 20rpx;
}

.my .card-box .card-container .card-bottom {
    margin: auto 24rpx 24rpx;
    height: 56rpx;
    width: 654rpx;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    flex-direction: row;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: justify;
    justify-content: space-between;
}

.my .card-box .card-container .card-bottom .text {
    font-size: 28rpx;
    color: #c9c9cd;
}

.my .card-box .card-container .card-bottom .text text {
    text-decoration: underline;
}

.my .card-box .card-container .card-bottom .balance {
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    flex-direction: row;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
    font-size: 28rpx;
    color: #f2f2f7;
}

.my .card-box .card-container .card-bottom .balance .nums {
    font-size: 40rpx;
    color: #f2f2f7;
}

.my .card-box .card-container .card-bottom .balance .icon {
    width: 39rpx;
    height: 39rpx;
    margin-left: 16rpx;
}

.my .card-box .card-container .card-bottom .btn-box {
    width: 120rpx;
    height: 56rpx;
    border-radius: 8rpx;
    background: #fcc800;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    flex-direction: row;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
    font-size: 26rpx;
    color: #333;
}

.my .panel-box {
    padding: 0 24rpx;
}

.my .panel {
    margin-top: -54rpx;
    border-radius: 16rpx;
    background-color: #333;
}

.my .panel .vip-box {
    border-radius: 0 0 16rpx 16rpx;
    padding: 24rpx 0 24rpx 24rpx;
    box-shadow: 0 8rpx 24rpx 0 rgba(0,0,0,0.32);
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-align: start;
    align-items: flex-start;
    -webkit-box-pack: start;
    justify-content: flex-start;
}

.my .panel .vip-box .level {
    width: 100%;
    display: flex;
