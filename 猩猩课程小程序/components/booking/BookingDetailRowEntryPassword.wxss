.entry-password-row {
    -webkit-box-flex: 1;
    flex: auto;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: justify;
    justify-content: space-between;
    padding: 32rpx 0;
}

.entry-password-row .label {
    font-size: 28rpx;
    color: #c9c9cd;
}

.entry-password-row .value {
    font-size: 28rpx;
    color: #f2f2f7;
}