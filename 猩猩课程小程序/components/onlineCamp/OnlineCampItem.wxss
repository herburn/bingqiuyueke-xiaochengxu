@import "../../__wuBaseWxss__/2.wxss";

@import "../../__wuBaseWxss__/0.wxss";

.online-camp-item-wrap {
    margin-bottom: 24rpx;
    border-radius: 16rpx;
    background: #333;
}

.online-camp-item-wrap .link {
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    flex-direction: row;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
    border-radius: 0 0 16rpx 16rpx;
    padding: 16rpx;
    font-size: 22rpx;
    color: #c9c9cd;
    text-align: center;
    line-height: 31rpx;
    background: #3f3f3f;
}

.online-camp-item-wrap .link .iconfont {
    font-size: 31rpx;
}