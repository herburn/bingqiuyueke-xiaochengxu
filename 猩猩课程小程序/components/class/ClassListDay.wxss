.class-empty {
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    flex-direction: row;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
    font-size: 28rpx;
    color: #767678;
    height: 168rpx;
    width: calc(100% - 48rpx);
    margin: 24rpx auto 0;
    border-radius: 16rpx;
    background-color: #333;
}