.banner {
    position: relative;
    width: 702rpx;
    height: 100%;
    border-radius: 16rpx;
    overflow: hidden;
}

.banner .img {
    width: 100%;
    height: 100%;
}

.banner .mask {
    position: absolute;
    top: 0;
    left: 0;
    z-index: 1;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.24);
}